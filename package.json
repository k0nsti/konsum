{
  "name": "konsum",
  "version": "0.0.0-semantic-release",
  "description": "",
  "keywords": [],
  "main": "dist/server.js",
  "module": "src/server.js",
  "bin": {
    "konsum-server": "bin/konsum-server"
  },
  "scripts": {
    "cover": "rollup -c tests/rollup.config.js && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "test": "mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=bin/konsum-server -c -- src/server.js && chmod +x bin/konsum-server",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish",
    "prestart": "npm run prepublish",
    "start": "node bin/konsum-server --config=config/config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k0nsti/konsum.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "config-expander": "^6.4.0",
    "koa": "^2.0.1",
    "koa-better-router": "^2.1.1",
    "koa-jwt": "^2.2.2",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "semantic-release": "^6.3.6"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=7.6"
  }
}
