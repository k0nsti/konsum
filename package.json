{
  "name": "konsum",
  "version": "0.0.0-semantic-release",
  "main": "dist/server.js",
  "module": "src/server.js",
  "description": "",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "konsum": "bin/konsum"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "test": "ava",
    "semantic-release": "semantic-release && npm publish && semantic-release post",
    "prepublish": "rollup --output=bin/konsum -c -- src/server.js && chmod +x bin/konsum",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prestart": "npm run prepublish",
    "start": "node bin/konsum --config=config/config.json",
    "prepare": "rollup -c",
    "docs": "documentation readme src/server.js --section=API",
    "lint": "documentation lint src/server.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^0.9.0",
    "config-expander": "^9.1.2",
    "koa": "^2.5.0",
    "koa-better-router": "^2.1.1",
    "koa-jwt": "^3.3.0",
    "sqlite3": "3.1.13"
  },
  "devDependencies": {
    "chai-http": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.55.5",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-istanbul": "^2.0.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.3",
    "semantic-release": "^13.1.4",
    "documentation": "^5.4.0",
    "nyc": "^11.5.0",
    "ava": "^0.25.0",
    "travis-deploy-once": "^4.3.4"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k0nsti/konsum.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
