{
  "name": "konsum",
  "version": "0.0.0-semantic-release",
  "description": "",
  "keywords": [],
  "main": "dist/server.js",
  "module": "src/server.js",
  "bin": {
    "konsum": "bin/konsum"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "test": "ava",
    "semantic-release": "semantic-release",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prestart": "npm run prepublish",
    "start": "node bin/konsum --config=config/config.json",
    "prepare": "rollup -c",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "undefined && chmod +x bin/konsum"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k0nsti/konsum.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum#readme",
  "dependencies": {
    "caporal": "^0.8.0",
    "config-expander": "^9.0.0",
    "koa": "^2.4.1",
    "koa-better-router": "^2.1.1",
    "koa-jwt": "^3.2.1",
    "sqlite3": "3.1.13"
  },
  "devDependencies": {
    "chai-http": "^3.0.0",
    "jsdoc-to-markdown": "^3.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.51.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semantic-release": "^9.1.1",
    "jsdoc-babel": "^0.3.0",
    "nyc": "^11.3.0",
    "ava": "^0.23.0",
    "xo": "^0.19.0"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.1"
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
