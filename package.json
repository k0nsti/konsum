{
  "name": "konsum",
  "version": "0.0.0-semantic-release",
  "description": "",
  "keywords": [],
  "main": "dist/server.js",
  "module": "src/server.js",
  "bin": {
    "konsum": "bin/konsum"
  },
  "scripts": {
    "cover": "nyc ava && mocha --reporter tests/istanbul.reporter.js build/bundle.test.js",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prestart": "npm run prepublish",
    "start": "node bin/konsum --config=config/config.json",
    "prepare": "rollup -c -- src/server.js && chmod +x bin/konsum",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k0nsti/konsum.git"
  },
  "bugs": {
    "url": "https://github.com/k0nsti/konsum/issues"
  },
  "homepage": "https://github.com/k0nsti/konsum#readme",
  "dependencies": {
    "caporal": "^0.7.0",
    "config-expander": "^9.0.0",
    "koa": "^2.2.0",
    "koa-better-router": "^2.1.1",
    "koa-jwt": "^3.2.1",
    "sqlite3": "^3.1.9"
  },
  "devDependencies": {
    "chai-http": "^3.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semantic-release": "^6.3.6",
    "jsdoc-babel": "^0.3.0",
    "nyc": "^11.1.0",
    "ava": "^0.21.0",
    "rollup-plugin-babel": "^2.7.1",
    "babel-preset-stage-3": "^6.24.1",
    "xo": "^0.19.0"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.2.1"
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": ["build/test-bundle.js"],
    "presets": ["stage-3"],
    "require": ["babel-register"]
  },
  "nyc": {
    "include": ["build/test-bundle.js"],
    "reporter": ["lcov"],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
