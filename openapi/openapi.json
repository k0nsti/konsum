{
  "openapi": "3.0.0",
  "info": {
    "title": "collect resource usage in a home environment",
    "version": "0.0.0-semantic-release"
  },
  "paths": {
    "/authenticate": {
      "post": {
        "description": "Login to request api token.",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "username",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "required": true,
            "type": "string"
          }
        ]
      }
    },
    "/category": {
      "get": {
        "description": "list notes of a category",
        "produces": [
          "application/json"
        ]
      },
      "parameters": [
        {
          "name": "category",
          "in": "path",
          "description": "ID of category to be listed",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "note",
          "in": "path",
          "description": "ID of note to be listed",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ],
      "put": {
        "produces": [
          "application/json"
        ]
      },
      "delete": {
        "produces": [
          "application/json"
        ]
      }
    },
    "/category/:category/value": {
      "parameters": [
        {
          "name": "category",
          "in": "path",
          "description": "ID of category from where the value bill deleted",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "time",
          "in": "body",
          "description": "time of the value",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ],
      "get": {
        "description": "List values of a category",
        "produces": [
          "application/json"
        ]
      },
      "post": {
        "description": "Insert a value into a category",
        "produces": [
          "application/json"
        ]
      },
      "delete": {
        "description": "Delete a value from a category",
        "produces": [
          "application/json"
        ]
      }
    }
  },
  "components": {},
  "tags": []
}