{
  "openapi": "3.0.0",
  "info": {
    "title": "collect resource usage in a home environment",
    "version": "0.0.0-semantic-release"
  },
  "paths": {
    "/authenticate": {
      "post": {
        "description": "Login to request api token.",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "username",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Token generated.",
            "content": {
              "application/json": {}
            }
          },
          "401": {
            "description": "Authentication failed.",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
    "/category": {
      "get": {
        "200": {
          "description": "A list of categories.",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/category"
                }
              }
            }
          }
        },
        "description": "retrieve list of categories.",
        "produces": [
          "application/json"
        ],
        "responses": null
      }
    },
    "/category/{category}": {
      "parameters": [
        {
          "name": "category",
          "in": "path",
          "description": "ID of category that needs to be deleted",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ],
      "put": {
        "description": "insert a new category",
        "produces": [
          "application/json"
        ]
      },
      "delete": {
        "description": "Delete a category.",
        "produces": [
          "application/json"
        ]
      }
    },
    "/category/{category}/value": {
      "parameters": [
        {
          "name": "category",
          "in": "path",
          "description": "ID of category from where the value bill deleted.",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "time",
          "in": "body",
          "description": "Time the value was present.",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ],
      "get": {
        "description": "List values of a category.",
        "produces": [
          "application/json"
        ]
      },
      "post": {
        "description": "Insert a value into a category.",
        "produces": [
          "application/json"
        ]
      },
      "delete": {
        "description": "Delete a value from a category.",
        "produces": [
          "application/json"
        ]
      }
    },
    "/category/{category}/meter": {
      "parameters": [
        {
          "name": "category",
          "in": "path",
          "description": "ID of category the meter belongs to.",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "mater",
          "in": "path",
          "description": "ID of meter to be updated.",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ],
      "get": {
        "description": "list meters of a category.",
        "produces": [
          "application/json"
        ]
      },
      "put": {
        "description": "Add a meter to a category.",
        "produces": [
          "application/json"
        ]
      },
      "post": {
        "description": "Update a meter.",
        "produces": [
          "application/json"
        ]
      }
    },
    "/category/{category}/note": {
      "parameters": [
        {
          "name": "category",
          "in": "path",
          "description": "ID of category to note belongs to.",
          "required": true,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "note",
          "in": "path",
          "description": "ID of note to be updated.",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ],
      "get": {
        "description": "list notes of a category.",
        "produces": [
          "application/json"
        ]
      },
      "put": {
        "description": "add a note to a category.",
        "produces": [
          "application/json"
        ]
      },
      "post": {
        "description": "Update a note.",
        "produces": [
          "application/json"
        ]
      }
    }
  },
  "components": {},
  "tags": []
}